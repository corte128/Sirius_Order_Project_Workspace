package com.sirius.employeews.jaxws;

import javax.jws.WebMethod;
import javax.jws.WebParam;
import javax.jws.WebResult;
import javax.jws.WebService;
import javax.xml.ws.RequestWrapper;
import javax.xml.ws.ResponseWrapper;

@WebService(name = "Employee", targetNamespace = "http://ace.sirius.com/profile/wsdl")
public interface Employee {
	
	@WebMethod(action = "getEmployeeById")
	@WebResult(name = "getEmployeeByIdReturn", targetNamespace = "http://ace.sirius.com/profile/wsdl")
	@RequestWrapper(localName = "getEmployeeById", targetNamespace = "http://ace.sirius.com/profile/wsdl", className = "com.sirius.ace.jaxws.GetEmployeeById")
	@ResponseWrapper(localName = "getEmployeeByIdResponse", targetNamespace = "http://ace.sirius.com/profile/wsdl", className = "com.sirius.ace.jaxws.GetEmployeeByIdResponse")
	public Employee getEmployeeById(
			@WebParam(name = "id", targetNamespace = "http://ace.sirius.com/profile/wsdl") int id);
	
	@WebMethod(action = "getEmployeeByUsername")
	@WebResult(name = "getEmployeeByUsernameReturn", targetNamespace = "http://ace.sirius.com/profile/wsdl")
	@RequestWrapper(localName = "getEmployeeByUsername", targetNamespace = "http://ace.sirius.com/profile/wsdl", className = "com.sirius.ace.jaxws.GetEmployeeByUsername")
	@ResponseWrapper(localName = "getEmployeeByUsernameResponse", targetNamespace = "http://ace.sirius.com/profile/wsdl", className = "com.sirius.ace.jaxws.GetEmployeeByUsernameResponse")
	public Employee getEmployeeByUsername(
			@WebParam(name = "username", targetNamespace = "http://ace.sirius.com/profile/wsdl") String username);
	
	@WebMethod(action = "deleteEmployeeProfile")
	@WebResult(name = "deleteEmployeeProfileReturn", targetNamespace = "http://ace.sirius.com/profile/wsdl")
	@RequestWrapper(localName = "deleteEmployeeProfile", targetNamespace = "http://ace.sirius.com/profile/wsdl", className = "com.sirius.ace.jaxws.DeleteEmployeeProfile")
	@ResponseWrapper(localName = "deleteEmployeeProfileResponse", targetNamespace = "http://ace.sirius.com/profile/wsdl", className = "com.sirius.ace.jaxws.DeleteEmployeeProfileResponse")
	public boolean deleteEmployeeProfile(
			@WebParam(name = "id", targetNamespace = "http://ace.sirius.com/profile/wsdl") int id);
	
	
}
